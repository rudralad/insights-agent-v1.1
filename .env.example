# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GROQ_API_KEY=your_groq_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# LLM Provider Configuration
# Options: openai, groq, gemini
LLM_PROVIDER=openai

# LLM Model Selection
# For OpenAI: o1-mini, gpt-3.5-turbo, gpt-4, etc.
# For Groq: llama3-70b-8192, mixtral-8x7b-32768, etc.
# For Gemini: gemini-1.5-pro, gemini-1.5-flash, etc.
LLM_MODEL=o1-mini

# User Agent for web scraping
USER_AGENT=myagent/1.0

# LangSmith Configuration (optional)
LANGSMITH_TRACING=false
LANGSMITH_ENDPOINT="https://api.smith.langchain.com"
LANGSMITH_API_KEY="your_langsmith_api_key_here"
LANGSMITH_PROJECT="your_langsmith_project_name"

# Airtable Configuration
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_airtable_base_id_here
AIRTABLE_USERS_TABLE_ID=your_airtable_users_table_id_here
AIRTABLE_RESEARCH_TABLE_ID=your_airtable_research_table_id_here
AIRTABLE_SEARCH_DATA_TABLE_ID=your_airtable_search_data_table_id_here
AIRTABLE_ANALYTICS_TABLE_ID=your_airtable_analytics_table_id_here 